        class AccessibleRelationType {
        public :
          enum x {
            CONTENT_FLOWS_FROM
            CONTROLLED_BY,
            CONTROLLER_FOR,
            LABELED_BY,
            LABEL_FOR,
            MEMBER_OF,
            NODE_CHILD_OF,
